<span class="active">
  <dsp:getvalueof var="selectedSize" param="selectedSize" />
  
  <c:choose>
    <c:when test="${not empty selectedSize}">      
      <span><c:out value="${selectedSize}" escapeXml="true"/></span>
    </c:when>
    <c:otherwise>
      
    </c:otherwise>
  </c:choose>
</span>

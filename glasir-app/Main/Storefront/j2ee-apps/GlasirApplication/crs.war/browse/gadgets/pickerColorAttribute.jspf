<span class="active">
  <dsp:getvalueof var="selectedColor" param="selectedColor"/>
  <c:choose>
    <c:when test="${not empty selectedColor}">
      <span><c:out value="${selectedColor}" escapeXml="true"/></span>
    </c:when>
    <c:otherwise>
       
    </c:otherwise>
  </c:choose>
</span>

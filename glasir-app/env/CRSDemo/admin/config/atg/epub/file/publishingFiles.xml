<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE gsa-template
        PUBLIC "-//Art Technology Group, Inc.//DTD General SQL Adapter//EN"
        "http://www.atg.com/dtds/gsa/gsa_1.0.dtd">

<gsa-template>
  <header>
    <name>PublishingFiles</name>
    <author>Marcus Pemer</author>
    <version></version>
  </header>
    <!-- Re-point the pathPrefix to our actual virtual file store location -->
    <!-- since otherwise our file store will be removed upon every build  -->
  <item-descriptor name="fileAsset">
    <property name="content">
      <attribute name="pathPrefix" value="{atg.dynamo.home}/Publishing/versionFileStore"/>
    </property>
  </item-descriptor>
</gsa-template>
